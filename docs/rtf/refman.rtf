71     {\cf17 struct }Net : {\cf17 public} torch::nn::Module \{\par
72     std::vector<torch::nn::Linear> fc;\par
73     \par
74     Net({\cf18 int} nLayers, {\cf18 int} nInput, {\cf18 bool} withBias = {\cf17 false}) \{\par
75         {\cf20 // calc an exp reduction of the numbers always reaching 1}\par
76         {\cf17 const} {\cf18 float} b = (float)exp(log(nInput)/(nLayers-1));\par
77         {\cf18 int} inputNeurons = nInput;\par
78         {\cf19 for}({\cf18 int} i=1;i<nLayers;i++) \{\par
79         {\cf18 int} outputNeurons = (int)ceil(nInput / pow(b,i));\par
80         {\cf19 if} (i == (nLayers-1)) outputNeurons = 1;\par
81         {\cf18 char} tmp[256];\par
82         sprintf(tmp,{\cf22 "fc%d_%d_%d"},i,inputNeurons,outputNeurons);\par
83         {\cf19 if} (debugOutput)\par
84             fprintf(stderr,{\cf22 "Creating FC layer: %s\\n"},tmp);\par
85         torch::nn::Linear ll = register_module(\par
86             tmp,\par
87             torch::nn::Linear(torch::nn::LinearOptions(inputNeurons, outputNeurons).bias(withBias))\par
88             );\par
89         torch::nn::init::xavier_uniform_(ll->weight,xavierGain);\par
90         {\cf19 if} (withBias) torch::nn::init::constant_(ll->bias, 0.0);\par
91         fc.push_back(ll);\par
92         {\cf19 if} (1 == outputNeurons) {\cf19 break};\par
93         inputNeurons = outputNeurons;\par
94         \}\par
95     \}\par
96 \par
97     torch::Tensor forward(torch::Tensor x, ActMethod am) \{\par
98         {\cf19 for}({\cf17 auto}& f:fc) \{\par
99         {\cf19 switch} (am) \{\par
100         {\cf19 default}:\par
101         {\cf19 case} Act_Tanh:\par
102             x = torch::atan(f->forward(x));\par
103             {\cf19 break};\par
104         {\cf19 case} Act_Sigmoid:\par
105             x = torch::sigmoid(f->forward(x));\par
106             {\cf19 break};\par
107         {\cf19 case} Act_ReLU:\par
108             x = torch::relu(f->forward(x));\par
109             {\cf19 break};\par
110         {\cf19 case} Act_NONE:\par
111             x = f->forward(x);\par
112             {\cf19 break};\par
113         \}\par
114         \}\par
115         {\cf19 return} x;\par
116     \}\par
117 \par
118     \};\par

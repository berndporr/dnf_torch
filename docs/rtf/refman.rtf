26 {\cf17 public}:\par
27     {\cf18 void} init({\cf18 size_t} delaySamples)\par
28     \{\par
29         delaySamples_ = delaySamples;\par
30         buffer_ = std::deque<float>(delaySamples_, 0.0f);\par
31     \}\par
32 \par
33     {\cf18 float} process({\cf18 float} input) \{\par
34         {\cf20 // Output is the oldest value (front of deque)}\par
35         {\cf18 float} output = buffer_.front();\par
36         buffer_.pop_front();\par
37     \par
38         {\cf20 // Push new input to the back}\par
39         buffer_.push_back(input);\par
40 \par
41         {\cf19 return} output;\par
42     \}\par
43 \par
44     {\cf18 float} get({\cf18 int} i){\cf17  const }\{\par
45     {\cf19 return} buffer_[i];\par
46     \}\par
47 \par
48     {\cf18 float} getNewest(){\cf17  const }\{\par
49     {\cf19 return} buffer_.back();\par
50     \}\par
